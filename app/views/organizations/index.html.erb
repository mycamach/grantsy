<h1>Organizations</h1>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.name %></td>
        <td><%= 'Show' %></td>
        <td><%= 'Edit' %></td>
        <td><%= 'Destroy' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
